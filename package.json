{
  "name": "plurch",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "electron dist/",
    "start.ng2": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build-electron.dev": "ng build && cp src/electron/* dist/",
    "build-electron.prod": "ng build --prod -aot --no-sourcemap && cp src/electron/* dist/",
    "electron.dev": "npm run build-electron.dev && electron dist/",
    "electron.prod": "npm run build-electron.prod && electron dist/",
    "install.node.modules": "cd dist/ && npm i",
    "pkg.mac": "npm run install.node.modules && electron-packager dist/ plurch --platform=mas --electronVersion=1.4.15 --overwrite",
    "pkg.win": "npm run install.node.modules && electron-packager dist/ plurch --platform=win32 --electron-version=1.4.15 --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.2",
    "@angular/common": "^4.1.2",
    "@angular/compiler": "^4.1.2",
    "@angular/core": "^4.1.2",
    "@angular/forms": "^4.1.2",
    "@angular/http": "^4.1.2",
    "@angular/material": "^2.0.0-beta.5",
    "@angular/platform-browser": "^4.1.2",
    "@angular/platform-browser-dynamic": "^4.1.2",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "express": "^4.14.1",
    "filepreview": "^1.0.12",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "loudness": "^0.2.2",
    "moment": "^2.17.1",
    "ng2-dragula": "^1.3.0",
    "rxjs": "5.0.1",
    "ts-helpers": "^1.1.1",
    "youtube-search": "^1.0.8",
    "ytdl-core": "^0.9.0",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.3",
    "@angular/compiler-cli": "^4.1.2",
    "@angular/platform-server": "^4.1.2",
    "@types/electron": "^1.4.30",
    "@types/node": "^6.0.42",
    "codelyzer": "~1.0.0-beta.4",
    "electron": "^1.4.15",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.9",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "~2.2.1",
    "webdriver-manager": "10.2.5"
  }
}
